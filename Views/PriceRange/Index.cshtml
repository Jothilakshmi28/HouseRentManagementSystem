@model HouseRentManagementSystem.Models.PriceRange

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@using(Html.BeginForm())
{<table>
    @for (int i = 0; i < Model.IsChecked.Count; i++)
    {
        <td>
        @Html.CheckBoxFor(Model => Model.Price[i].IsChecked)
        </td>
        }
    }
</table>